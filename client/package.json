{
  "name": "mm",
  "version": "14.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build-functions": "(cd functions && npm run build)",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "deploy": "gcloud config set functions/region europe-west1 --project my-mechanic-dev",
    "deploy:dev": "ng build && firebase use my-mechanic-dev && firebase deploy",
    "deploy:dev:functions": "npm run build-functions && firebase use my-mechanic-dev && firebase deploy --only functions",
    "pg:watch": "postgraphile -c \"postgres:///mm-dev\"   --subscriptions --watch --dynamic-json --no-setof-functions-contain-nulls --no-ignore-rbac --no-ignore-indexes --show-error-stack=json --extended-errors hint,detail,errcode --append-plugins @graphile-contrib/pg-simplify-inflector --export-schema-graphql schema.graphql --graphiql \"/\" --enhance-graphiql --allow-explain --enable-query-batching --legacy-relations omit \\  --schema app_public"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0",
    "@angular-devkit/build-angular": "~0.900.0-rc.3",
    "@angular-material-extensions/google-maps-autocomplete": "^2.0.0",
    "@angular/animations": "~9.0.0-rc.3",
    "@angular/cdk": "~8.2.3",
    "@angular/cli": "~9.0.0-rc.3",
    "@angular/common": "~9.0.0-rc.3",
    "@angular/compiler": "~9.0.0-rc.3",
    "@angular/compiler-cli": "~9.0.0-rc.3",
    "@angular/core": "~9.0.0-rc.3",
    "@angular/forms": "~9.0.0-rc.3",
    "@angular/language-service": "~9.0.0-rc.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.0-rc.3",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.3",
    "@angular/router": "~9.0.0-rc.3",
    "@ng-reactive/async-input": "^0.0.1",
    "@types/googlemaps": "^3.38.0",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "codelyzer": "^5.1.2",
    "date-fns": "^2.7.0",
    "graphql": "^14.5.0",
    "graphql-tag": "^2.10.0",
    "h3-js": "^3.6.1",
    "hammerjs": "^2.0.8",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lodash-es": "^4.17.15",
    "ngx-json-viewer": "^2.4.0",
    "ngx-reactivetoolkit": "^6.1.1",
    "prettier": "^1.18.2",
    "protractor": "~5.4.2",
    "rxjs": "~6.5.3",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "~3.6.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {}
}
